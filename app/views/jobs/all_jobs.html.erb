<h1>All Jobs</h1>

<% @jobs.each do |job| %>
  <ul>
    <li><%=link_to job.title, company_job_path(job.company, job)%> at
        <%=link_to job.company.name, company_path(job.company)%> in
        <%= job.city %></br>
        <%="Rating: #{job.level_of_interest}"%>
  </ul>
<% end %>
<p><%= link_to "See All Companies", companies_path%></p>
