<h3> Dashboard </h3>

<p>Jobs by Interest</p>
<% @group_by_interest.each do |interest_level, count| %>
<p><%= "#{interest_level} Star Rating: (#{count}) Job(s)"  %></p>
<% end %>

<p>Top Companies By Interest</p>
<% @top_three.each do |company| %>

<p><%= company.name %> (<%= company.avg_of_interest %>)</p>
<% end %>

<% @city_count.each do |city, count| %>
<%= link_to "#{city}", "/jobs?location=#{city}" %><%= "(#{count} jobs)"  %>
<% end %>
